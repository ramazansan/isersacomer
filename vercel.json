{
    "version": 2,
    "outputDirectory": "public",
    "builds": [
        {
            "src": "public/index.php",
            "use": "vercel-php@0.7.0"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://isersacomer.vercel.app",
        "APP_KEY": "base64:YOUR_GENERATED_KEY_HERE"
    }
}